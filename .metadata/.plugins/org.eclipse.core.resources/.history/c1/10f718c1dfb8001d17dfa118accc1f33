package chap1_CJH;

public class Solution02 {

	public static void main(String[] args) {

		int[] nums = {4, 2, 6, 1, 3};
		
		
		String star = "*";
		
		while(true) {
			for(int i=0; i<nums.length; i++) {
				System.out.print(star);
			}
			break;
		}
		
		
		
		
//		for(int i=0; i<nums.length; i++) {
//		
//			}
		
		
		
		
		
	}

}
