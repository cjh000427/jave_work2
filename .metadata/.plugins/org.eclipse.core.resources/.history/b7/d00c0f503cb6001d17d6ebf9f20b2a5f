package basic.array;

public class ArrayDelete {

	public static void main(String[] args) {

		int[] arr = {1, 3, 5, 7, 9, 11, 13};
		
//		arr[3] = arr[4];
//		arr[4] = arr[5];
//		arr[5] = arr[6];
		
		for(int i=3; i<arr.length-1; i++) { //시작값은 삭제하려는 값
			arr[i] = arr[i+1];
		}
		
		
		
		

	}

}













