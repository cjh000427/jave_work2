package oop.static_.field;

public class MainClass {

	public static void main(String[] args) {

		Count c1 = new Count();
		
		c1.a += 5;
		c1.b += 5;
		
		System.out.println("인스턴스 변수 c1.a: " + c1.a); 
		System.out.println("정적 변수 c1.b: " + c1.b); 
		
		System.out.println("-------------------------------");
		
		Count c2 = new Count();
		
		c2.a += 7;
		c2.b += 7;
		
		System.out.println("인스턴스 변수 c1.a: " + c1.a); 
		System.out.println("정적 변수 c1.b: " + c1.b); 

		
		
		
		
	}

}






