package etc.api.random;

import java.util.Random;

public class RandomExample {

	public static void main(String[] args) {

		Random r = new Random();
		
		//실수 난수: 0.0 <= ~ <1.0
		double d = r.nextDouble();
		System.out.println(d);
		
		//정수 난수: nextInt();
		int i = r.nextInt();
		System.out.println(i);
		
		
		
		
		
		
		
		
	}

}












