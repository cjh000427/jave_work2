package oop.poly.casting;

public class MainClass {

	public static void main(String[] args) {

		Parent p = new Parent();
		p.n1 = 1;
//		p.n2 = 2; (x)
		
		p.method1();
		p.method2();
//		p.method3(); (x)
		
		System.out.println("----------------------------------");
		
		Child c = new Child();
		c.n1 = 1; //부모에게 물려받은 속성
		c.n2 = 2; //자식의 고유 속성
		
		c.method1(); 
		c.method2();
		c.method3();
		
		
		
	}

}





