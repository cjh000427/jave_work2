package chap2_CJH.quiz01;

public class Person {

	private String age;
	private String name;
	
	public Person(String name, String age) {
		this.name = name;
		this.age = age;
	}
	
	public String info(String name, String age) {
		this.name = name;
		this.age = age;
		return "이름: " + name + "나이: " + age;
	}
	
	
	
}
